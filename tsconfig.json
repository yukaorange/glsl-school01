{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "noImplicitAny": false,
    "noEmitOnError": false,
    "noEmit": true,
    "allowJs": true,
    "checkJs": false,
    "baseUrl": ".",
    "paths": {
      "@/components/*": ["src/components/*"],
      "@/projects/*": ["src/projects/*"],
      "@/layouts/*": ["src/layouts/*"],
      "@/images/*": ["src/images/*"],
      "@/js/*": ["src/js/*"],
      "@/styles/*": ["src/styles/*"]
    }
  }
}
